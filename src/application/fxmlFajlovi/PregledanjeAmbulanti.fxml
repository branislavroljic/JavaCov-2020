<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" prefHeight="454.0" prefWidth="360.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TableView fx:id="table" prefHeight="372.0" prefWidth="360.0">
         <columns>
            <TableColumn fx:id="id" prefWidth="120.0" text="Ambulanta ID" />
            <TableColumn fx:id="kapacitet" prefWidth="120.0" text="Kapacitet" />
            <TableColumn fx:id="popunjenost" prefWidth="120.0" text="Popunjenost" />
         </columns>
      </TableView>
      <Button mnemonicParsing="false" onAction="#novaAmbulantaPressed" prefHeight="49.0" prefWidth="194.0" text="Nova ambulanta">
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Button>
      <Label fx:id="infoLabel" alignment="CENTER" prefHeight="23.0" prefWidth="342.0">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
   </children>
</VBox>
